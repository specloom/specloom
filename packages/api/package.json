{
  "name": "@specloom/api",
  "version": "0.1.0",
  "description": "OpenAPI spec for specloom admin API",
  "type": "module",
  "main": "./dist/openapi.json",
  "exports": {
    ".": "./dist/openapi.json",
    "./openapi.json": "./dist/openapi.json"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsp compile .",
    "dev": "tsp compile . --watch",
    "typecheck": "tsp compile . --no-emit"
  },
  "keywords": ["specloom", "openapi", "api", "typespec"],
  "license": "MIT",
  "peerDependencies": {
    "@typespec/compiler": ">=0.60.0"
  },
  "devDependencies": {
    "@typespec/compiler": "^0.64.0",
    "@typespec/http": "^0.64.0",
    "@typespec/openapi": "^0.64.0",
    "@typespec/openapi3": "^0.64.0"
  }
}
