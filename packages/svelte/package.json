    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "vaul-svelte": "1.0.0-next.7"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.21.0",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
